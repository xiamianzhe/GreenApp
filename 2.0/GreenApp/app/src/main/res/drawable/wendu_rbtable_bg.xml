<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/radiobgcolor"></solid>
            <corners android:topRightRadius="20dp" android:bottomRightRadius="20dp"/>
            <stroke android:width="1dp" android:color="@color/radiobgcolor"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/bg_main"></solid>
            <corners android:topRightRadius="20dp" android:bottomRightRadius="20dp"/>
            <stroke android:width="1dp" android:color="@color/radiobgcolor"/>
        </shape>
    </item>
</selector>